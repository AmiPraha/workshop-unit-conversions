{
    "name": "ami-praha/ami-praha-workshop-unit-conversions",
    "description": "test description",
    "keywords": [
        "amipraha",
        "ami-praha",
        "workshop-unit-conversions"
    ],
    "homepage": "https://github.com/AmiPraha/workshop-unit-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Miloslav Janou≈°ek",
            "email": "miloslav.janousek@ami.cz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "AmiPraha\\WorkshopUnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AmiPraha\\WorkshopUnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
